page{
  background-color: #F3F4F6;
}
.head-div{
  height: 130rpx;
  text-align: center;
  background: #ffffff;
  width: 100%;
  position: relative;
  top: 0;
}
.head-div .titleText{
  color: #000000;
  font-weight: bold;
  font-size: 37rpx;
  text-align: center;
  display: inline-block;
  padding-top: 67rpx;
}
.wrap{
  padding: 20rpx 24rpx;
  /* margin-top: 130rpx; */
}
.wrap .items{
  padding: 24rpx;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
  width: calc(100% - 48rpx);
  display: inline-block;
  text-align: left;
  margin-bottom: 30rpx;
}
.wrap .items .title{
  color: #333333;
  font-size: 32rpx;
}
.wrap .items .title .type-red{
  width:80rpx;
  height:32rpx;
  background:rgba(255,0,0,1);
  border-radius:8rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  text-align: center;
  line-height: 32rpx;
  display: inline-block;
}
.wrap .items .title .type{
  width:108rpx;
  height:32rpx;
  background:rgba(144,144,144,1);
  border-radius:8rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  text-align: center;
  line-height: 32rpx;
  display: inline-block;
}
.wrap .items .title .type.red{
  background:rgba(255,0,0,1);
}
.wrap .items .title .total{
  color: #909090;
  font-size: 24rpx;
  float: right;
}
.wrap .items .Request{
  height:24rpx;
  font-size:24rpx;
  font-weight:500;
  color:rgba(255,0,0,1);
  line-height:18rpx;
  margin-top: 25rpx;
}
.wrap .items .start{
  color: #333333;
  font-size: 24rpx;font-weight: 500;
  margin-top: 14rpx
}
.wrap .items .start text{
  color: #909090;
  margin: 0 13rpx;
  width: 1rpx;
}
.wrap .items .time{
  color: #333333;
  font-size: 24rpx;
  font-weight:500;
  margin-top: 14rpx;
}
.wrap .items image{
  margin-top: 28rpx;
  width: 652rpx;
  /* height: 230rpx; */
  object-fit: cover;
}
.wrap .items .prompt{
  width:150rpx;
  height:54rpx;
  border:1rpx solid rgba(240,167,16,1);
  border-radius:27rpx;
  color: #F0A710;
  font-size: 26rpx;
  text-align: center;
  line-height: 54rpx;
  display: inline-block;
  float: right;
  margin-top: 15rpx;

}