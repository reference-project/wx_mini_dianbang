.tc-show{
  width: 100%;
  position: fixed;
  z-index: 112;
  top: 0;
  bottom: 0;
  background: url(http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Ftk_yd_icon.png)no-repeat center top 100rpx;
  background-size: 70%;
}
.show-box{
  position: fixed;
  right: 0;
  left: 0;
  bottom: 0;
  background: #fff;
  z-index: 112;
  border-radius:40rpx 40rpx 0rpx 0rpx;
}
.show-box icon{
  background: url(http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Foff_tc_icon.png)no-repeat center;
  width: 24rpx;
  height: 22rpx;
  display: block;
  background-size: 22rpx 20rpx;
  position: absolute;
  right: 32rpx;
  top: 32rpx;
  }
.show-box .top-view{
  padding: 24rpx 24rpx 0;
}
.show-box .top-view image{
  width: 165rpx;
  height: 165rpx;
  object-fit: over;
  vertical-align: top;
  margin-right: 29rpx;
}
.show-box .top-view .content{
  display: inline-block;
  width: 450rpx;
  text-align: left;
}
.show-box .top-view .content .money{
  color: #FF0000;
  font-size: 32rpx;
  margin-bottom: 10rpx;
  margin-top: 10rpx;
  
}
.show-box .top-view .content .money>text{
  font-size:26rpx;
  font-weight:500;
  text-decoration:line-through;
  color:rgba(144,144,144,1);
  line-height:36rpx;
}
.show-box .top-view .content .num{
  font-size:24rpx;
  font-weight:500;
  color:rgba(144,144,144,1);
  line-height:36rpx;
  margin-bottom: 14rpx;
}
.show-box .top-view .content .choise{
  font-size:24rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:36rpx
}
.show-box .box-btn{
  padding: 60rpx 24rpx 0;
}
.show-box .box-btn .title{
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:36rpx;
  margin-bottom: 24rpx;
}
.show-box .box-btn .type{
  padding: 0 30rpx;
  height:64rpx;
  background:rgba(242,242,242,1);
  border-radius:32rpx;
  line-height: 64rpx;
  color: #333333;
  font-size: 26rpx;
  text-align: center;
  display: inline-block;
  font-weight:500;
  margin-right: 24rpx;
  margin-bottom: 24rpx;
}
.show-box .box-btn .type.active{
  background:rgba(240,167,16,0.05);
  border:1rpx solid rgba(240,167,16,1);
  color:rgba(240,167,16,1);
}
.show-box .amount{
  padding: 46rpx 24rpx 0;
  text-align: left;
  position: relative;
  margin-bottom: 225rpx;
}
.show-box .amount .sum{
  font-size:32rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:36rpx;
}
.show-box .amount .step view.num{
  background: #F7F7F7;
  color: #4C2D19;
  font-size: 28rpx;
  font-weight: bold;
}
.show-box .amount .step view{
  display:inline-block;
  width:50rpx;
  line-height:50rpx;
  height:50rpx;
  text-align:center;
}
.show-box .amount .step{
  position: absolute;
  right: 10rpx;
  text-align: right;
  margin-top: -5rpx;
}
.show-box .submit{
  width: 100%;
  position: absolute;
  bottom: 0;
  height: 100rpx;
}
.show-box .submit>view{
  color: #4C2D19;
  background:rgba(251,203,103,1);
  font-size: 28rpx;
  text-align: center;
  line-height: 100rpx;
  display: inline-block;
}
.show-box .submit .black{
  color: #FBCB67;
  background: #333333;

}
.gg{
  width: 100rpx;
height: 100rpx;
}
.dialog-sort{
    padding: 0.31rem 0 0.3rem 0;
    margin: 0 0.22rem;
}

.dialog-sort .title {
    font-size: 0.26rem;
    line-height: 0.28rem;
    margin-bottom: 0.3rem;
    color: #323232;
}
.sort-list{
    display: flex;
    flex-wrap: wrap;
}
.sort-item {
    font-size: 0.23rem;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    margin-right: 0.24rem;
    margin-bottom: 0.2rem;
    padding: 0.07rem 0.15rem 0.08rem 0.15rem;
    border-radius: 0.1rem;
    border: 0.01rem solid #f5f5f5;
    background-color: #f5f5f5;
    text-align: center;
}

button[disabled]{
background: gainsboro !important;
color:#333333 !important;

