  background-size: 36rpx 32rpx;
  box-shadow: 0rpx 2rpx 27rpx 0rpx rgba(0, 0, 0, 0.11);
  border-radius:50%;
  position: relative;
  z-index: 2

}
.float .top{
  width: 80rpx;
  height: 80rpx;
  background: url(http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Fzhiding%402x.png)no-repeat center  #ffffff;
  background-size: 36rpx 32rpx;
  box-shadow: 0rpx 2rpx 27rpx 0rpx rgba(0, 0, 0, 0.11);
  border-radius:50%;
  margin-bottom: 42rpx;
  margin-top: 24rpx;
  position: relative;
  z-index: 2
}
