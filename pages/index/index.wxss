.head-div .adress{
  background: url('http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Fgj_dw_icon(2).png')no-repeat left 24rpx center;
  background-size: 22rpx 26rpx;
