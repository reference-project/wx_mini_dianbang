  top: 159rpx;
  padding-left: 57rpx;
  left: 0;
  color: #4C2D19;
  font-size: 28rpx;
}
.head-div .search-btn{
  color: #4C2D19;
  font-size: 28rpx;
  float: right;
  position: absolute;
  top: 61rpx;
  right: 24rpx;
}
.head-div .search-box{
  float: left;
  position: absolute;
  left: 24rpx;
  top: 40rpx;
  width:625rpx;
  height:64rpx;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(255,255,255,1);
  border-radius:32rpx;
  line-height: 64rpx;
  background: url('http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Fsearch2.png')no-repeat center left 30rpx #ffffff;
  background-size: 27rpx;
  text-align: left;
}
.head-div .search-box input{
  border: none;
  height: 64rpx;
  line-height: 64rpx;
  margin-left: 70rpx;
  width: 440rpx;
  color: #333333;
  font-size: 24rpx;
}
.head-div .search-box input::placeholder{
  color: #909090;
  font-weight:400;
}
.head-div .search-box input::-webkit-input-placeholder{
  color: #909090;
  font-weight:400;
}
.user-box{
  margin: 0 24rpx;
  padding: 24rpx 21rpx;
  border-radius: 20rpx;
  width: calc(100% - 90rpx);
  box-shadow:0px 3px 27px 0px rgba(251,203,103,0.26);
  text-align: left;
  margin-top: -138rpx;
  position: relative;
  background: url(http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Fjr_copy.png)no-repeat center right 24rpx #ffffff;
  background-size: 17rpx 30rpx
}
.user-box image{
  width:120rpx;
  height:120rpx;
  border-radius:50%;
  vertical-align: top;
  margin-right: 29rpx;
}
.user-box .content{
  display: inline-block;
  width: 450rpx;
}
.user-box .content .name{
  color: #333333;
  font-size: 32rpx;
  font-weight:bold;
}
.user-box .content .name .address{
  color: #808080;
  font-size: 32rpx;
  margin-left: 10rpx;
  margin-right: 20rpx;
  font-weight:bold;
}
.user-box .content .name .mark{
  background: #FF0000;
  width:70rpx;
  height:32rpx;
  border-radius:5rpx;
  display: inline-block;
  line-height: 32rpx;
  color: #FFFFFF;
  font-size: 24rpx;
  text-align: center;
  font-weight:bold;
}
.user-box .content .num{
  color: #333333;
  font-weight: 500;
  font-size: 24rpx;
  margin-top: 10rpx;
}
.user-box .content .signature{
  color: #333333;
  font-size: 24rpx;
  font-weight:400;
  margin-top: 10rpx;

}
.list-box{
  margin: 20rpx 24rpx 0;
  background:rgba(255,255,255,1);
  border-radius:20rpx;
  width: calc(100% - 48rpx);
}
.list-box image{
  width:100%;
  height:248rpx;
  border-radius:20rpx 20rpx 0px 0px;
}
.list-box .bottom{
  width: 100%;
  padding-bottom: 20rpx;
  margin-top: 10rpx;
}
.list-box .bottom text{
  display: inline-block;
  vertical-align: top;
  /* margin-top: 40rpx; */
  width: 33.3%;
  text-align: center;
  font-size: 24rpx;
  color: #909090;
  padding-top: 95rpx;
   
}
.list-box .bottom .xsms{
  background: url(http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Fxsms_icon.png) no-repeat center top;
  background-size: 80rpx;
 
}
.list-box .bottom .jrsx{
  background: url(http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Fjrsx_icon.png) no-repeat center top;
  background-size: 80rpx;
}
.list-box .bottom .bkzj{
  background: url(http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Fbkzj_icon.png) no-repeat center top;
  background-size: 80rpx;
}
.wrap{
  margin: 20rpx 24rpx 20rpx;
  background: #ffffff;
  padding: 23rpx 24rpx;
  width: clac(100% - 96rpx);
  text-align: left;
}
.wrap .top-box{
  margin-bottom: 40rpx;
}
.wrap .top-box .title{
  color: #333333;
  font-size: 32rpx;

}
.wrap .top-box .title text{
  float: right;
  color: #909090;
  font-size: 24rpx;
}
.wrap .top-box .end{
  color: #333333;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.wrap .top-box .end .time:nth-child(1){
  margin-left: 5rpx;
}
.wrap .top-box .end .time{
  display: inline-block;
  width:130rpx;
  height:26rpx;
  line-height: 26rpx;
  /* background:rgba(51,51,51,1); */
  color: #F0A710;
  font-size: 24rpx;
  text-align: center;
  font-weight:500;

}
.wrap .top-box .end .line{
  color: #909090;
  width: 1rpx;
  margin: 0 14rpx;
}
.wrap .items{
  width: 100%;
  margin-bottom: 60rpx;
  position: relative;
  display: inline-block;
}
.wrap .items:last-child{
  margin-bottom: 0;
}
.wrap .items image{
  width: 200rpx;
  height: 200rpx;
  margin-right: 25rpx;
  vertical-align: top;
}
.wrap .items .content{
  width: 420rpx;
  display: inline-block;
}
.wrap .items .content .name{
  color: #333333;
  font-size: 28rpx;
  margin-top: 10rpx;
}
.wrap .items .content .money{
  color: #FF0000;
  font-weight: bold;
  font-size: 36rpx;
  margin-top: 20rpx;
}
.wrap .items .content .money text{
  text-decoration:line-through;
  color: #909090;
  font-size: 24rpx;
}
.wrap .items .content .money icon{
  background: url(http://zhishan-test.oss-cn-beijing.aliyuncs.com/dianbang-gg%2Fimage%2Ftj_gwc_icon.png)no-repeat center center;
  background-size: 33rpx 30rpx;
  width: 35rpx;
  height: 35rpx;
  display: inline-block;
  float: right;
  margin-top: 8rpx;
}
.float{
  position: fixed;
  right: 24rpx;
  bottom: 100rpx;
  z-index: 5;
  display: inline-block;
  /* background: #ffffff; */
}
.float .share{
  width: 80rpx;
  height: 80rpx;
